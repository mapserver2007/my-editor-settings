[
    { "key": "cmd+d",                  "command": "editor.action.deleteLines",
                                        "when": "editorTextFocus && !editorReadonly" },
    { "key": "cmd+m",                  "command": "markdown.showPreview",
                                        "when": "editorLangId == 'markdown'" }, 
    { "key": "cmd+shift+m",            "command": "markdown.showPreviewToSide",
                                        "when": "editorLangId == 'markdown'" }, 
    { "key": "cmd+l",                  "command": "workbench.action.gotoLine" }, 
    { "key": "cmd+shift+right",        "command": "workbench.action.nextEditor" },
    { "key": "cmd+shift+left",         "command": "workbench.action.previousEditor" },
]